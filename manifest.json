{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "dogeify",
  "version": "0.0",
  "manifest_version": 2,
  "description": "dogeifys all websites",

	"permissions": [
	"tabs",
	"http://*/*",
	"https://*/*"
	],
 "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-1.8.0.min.js","background.js","setfont.js"],
	  "css": ["setfont.css"],
	  "all_frames": true
	  
    }
  ]
}